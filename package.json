  "scripts": {
    "build": "npm run clean-dist && npm run build:css  && webpack -p --config=configs/webpack/prod.js",
    "clean-dist": "rimraf dist/*",
    "lint": "tslint './src/**/*.ts*' --format stylish --project . --force",
    "start": "npm run start-dev",
    "start-dev": "npm run build:css && webpack-dev-server --config=configs/webpack/dev.js",
    "start-prod": "npm run build && node express.js",
    "test": "NODE_ENV=test && jest  --coverage --config=configs/jest.json",
    "test:watch": "NODE_ENV=test && jest --watch --coverage --config=configs/jest.json",
    "build:css": "tailwind build src/style.css -o src/App.css",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
